{{#paper-toolbar}}
      <div class="md-toolbar-tools">
        {{#paper-sidenav-toggle as |toggle|}}
          {{#paper-button target=toggle action="toggleMenu" icon-button=true}}
            {{paper-icon icon="menu" size="lg"}}
          {{/paper-button}}
        {{/paper-sidenav-toggle}}


        <h2>RÃ¤ume</h2>

        {{paper-input label="Suche" value=filterString}}
      </div>
{{/paper-toolbar}}


{{#paper-content classNames="md-whiteframe-z1"}}
    {{#paper-list}}
      {{#each model as |item|}}
      {{#if item.visible}}
        {{#link-to 'timetable' 'room' item.id class="no-style"}}
          {{#paper-item}}
              <h3><b>{{item.name}}</b>  - {{item.longName}} {{item.foreName}}</h3>
          {{/paper-item}}
          {{paper-divider}}
          {{/link-to}}
        {{/if}}
      {{/each}}
    {{/paper-list}}
  {{/paper-content}}
